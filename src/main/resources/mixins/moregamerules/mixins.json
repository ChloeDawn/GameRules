{
  "target": "@environment(DEFAULT)",
  "minVersion": "0.7.11",
  "compatibilityLevel": "JAVA_8",
  "required": true,
  "package": "io.github.chloedawn.gamerules.mixin",
  "mixins": [
    "BooleanRuleFactory",
    "GameRulesAccessor",
    "GameRulesRegistrar",
    "IntRuleFactory",
    "RuleTypeFactoryAccessor",
    "RuleTypeFactoryMixin"
  ],
  "refmap": "$refmap",
  "injectors": {
    "defaultRequire": 1
  },
  "overwrites": {
    "conformVisibility": true,
    "requireAnnotations": true
  }
}
